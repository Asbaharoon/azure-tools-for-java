<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="com.microsoft.azure.toolkit.eclipse.explorer">
      <explorerNodeProvider
            implementation="com.microsoft.azure.toolkit.ide.springcloud.SpringCloudNodeProvider">
      </explorerNodeProvider>
   </extension>
   <extension
         point="com.microsoft.azure.toolkit.eclipse.actions">
      <actionContributor
            implementation="com.microsoft.azure.toolkit.ide.springcloud.SpringCloudActionsContributor">
      </actionContributor>
      <actionContributor
            implementation="com.microsoft.azure.toolkit.eclipse.springcloud.EclipseSpringCloudActionsContributor">
      </actionContributor>
   </extension>
</plugin>
